{
  "name": "devenpateldp/ost-sdk-php",
  "homepage": "https://kit.ost.com/",
  "description": "The official OST PHP SDK",
  "keywords": ["ost","simpletoken","sdk","php"],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true  
  "require": {
    "php": ">5",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Lib\\": "src/Lib/",
      "OST\\": "src/Services/",
      "": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
